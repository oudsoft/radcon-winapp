{
  "name": "electron-express",
  "productName": "Radconnext",
  "version": "1.0.0",
  "description": "Example of embedding a node express server within an electron app",
  "main": "main.js",
  "engine": {
    "node": ">=10.15.1"
  },
  "scripts": {
